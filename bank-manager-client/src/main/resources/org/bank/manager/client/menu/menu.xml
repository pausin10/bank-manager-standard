		<MenuSeparator />
		<MenuItem attr="FontSetup"
			icon="com/ontimize/gui/images/font.png" />
		<Menu attr="Font">
			<RadioItemGroup>
				<RadioMenuItem attr="SmallFont" />
				<RadioMenuItem attr="RegularFont" />
				<RadioMenuItem attr="LargeFont" />
			</RadioItemGroup>
		</Menu>
		<MenuSeparator />
		<MenuItem attr="ChangeTableFieldLook"
			icon="com/ontimize/gui/images/componentaspect.png" />
