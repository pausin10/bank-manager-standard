	</Menu>
	<Menu attr="Menumanageraccounts">
		<MenuItem attr="Manageraccounts"></MenuItem>
	</Menu>
	<Menu attr="Menumanagercustomers">
		<MenuItem attr="Managercustomers"></MenuItem>
	</Menu>

	<Menu attr="Menumanagerprofiles">
