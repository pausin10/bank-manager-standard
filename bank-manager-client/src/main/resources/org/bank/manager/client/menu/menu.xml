<?xml version="1.0" encoding="ISO-8859-1" ?>
<ApplicationMenuBar>
	<Menu attr="Administracion">
		<MenuItem attr="SendToTray" shortcut="ctrl T"
			icon="com/ontimize/gui/images/sendtotray.png" />
		<MenuSeparator />
		<MenuItem attr="ChangePassword" />
		<MenuSeparator />
		<MenuItem attr="Managerabout" />
		<MenuSeparator />
		<MenuItem attr="Lock"
			icon="com/ontimize/gui/images/padlock.png" shortcut="ctrl B" />
		<MenuItem attr="Exit"
			icon="com/ontimize/gui/images/exit2.png" />
	</Menu>

	<MenuLocale attr="Language"
